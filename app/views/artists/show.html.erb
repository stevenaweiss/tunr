<h1><%= @artist.name %></h1>
<p><img src="<%= @artist.photo_url%>" width="500"/></p>
<ul>
  <%@songs.each do |song| %>
  <li><%= song.title %>, <%= song.year %></li>
  <%= link_to "Edit This Song!", edit_artist_song_path(@artist, song)%>


   <form action="/artists/<%= @artist.id %>/songs/<%= song.id %>" method="POST">
  <input type="hidden" name="_method" value="DELETE" />
  <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />

  <button>Delete This Song!</button>
</form>


  <% end %>
  </ul>

  <%= link_to "Add a song!", new_artist_song_path(@artist)%>

  <a href="/artists">Back To Artists</a>




