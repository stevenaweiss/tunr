<h1>Artists</h1>
<ul>
  <% @artists.each do |artist| %>
  <a href="/artists/<%= artist.id %>">
  <li><%= artist.name %></li>
  <li><%= artist.genre %></li>
  <li><img src="<%= artist.photo_url %>" width="400"></li>
  </a>
  <a href="/artists/<%= artist.id %>/edit">Edit This Artist</a>

  <form action="/artists/<%= artist.id %>" method="POST">
  <input type="hidden" name="_method" value="DELETE" />
  <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />

  <button>Delete This Artist!</button>
</form>
<%end %>
</br>

<a href="/artists/new">Create a New Artist</a>
</ul>